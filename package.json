{
  "name": "vscode-relative-motion",
  "displayName": "Relative Motion",
  "description": "Vaguely vim-like cursor-relative line jumping",
  "version": "0.1.0",
  "categories": [
    "Other"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/pyrrho/vscode-relative-motion.git"
  },
  "bugs": {
    "url": "https://github.com/pyrrho/vscode-relative-motion/issues"
  },
  "author": "Drew Pirrone-Brusse",
  "publisher": "Drew Pirrone-Brusse",
  "license": "MIT",
  "engines": {
    "vscode": "^1.84.0"
  },
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "vscode-relative-motion.up",
        "title": "Relative Motion Up"
      },
      {
        "command": "vscode-relative-motion.down",
        "title": "Relative Motion Down"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts"
  },
  "devDependencies": {
    "@types/vscode": "^1.84.0",
    "@types/node": "18.x",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "eslint": "^8.52.0",
    "typescript": "^5.2.2"
  }
}
